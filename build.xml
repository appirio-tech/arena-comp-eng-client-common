<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="client-common" basedir="." default="publish-workspace">
    <property name="root.dir" location="${basedir}/../.." />
    <property name="glue.dir" location="${root.dir}/glue" />

    <property name="compile.source" value="1.4" />
    
    <import file="${glue.dir}/glue-jar.xml" />
    
    <target name="compile-1.5" depends="prepare-compile">
        <compile target="1.5">
            <includes>
                <include name="com/topcoder/client/connectiontype/ChinaProxySelector**" />
            </includes>
        </compile>
    </target>

    <target name="compile" depends="compile-1.5, glue-jar.compile" />
</project>
